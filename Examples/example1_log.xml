<?xml version="1.0" encoding="UTF-8" ?>
<root>
    <map>
        <width>2</width>
        <height>2</height>
        <cellsize>24</cellsize>
        <startx>0</startx>
        <starty>1</starty>
        <finishx>1</finishx>
        <finishy>0</finishy>
        <grid>
            <row>1 0</row>
            <row>0 0</row>
        </grid>
    </map>
    <algorithm>
        <searchtype>astar</searchtype>
        <metrictype>diagonal</metrictype>
        <breakingties>g-max</breakingties>
        <hweight>1</hweight>
        <allowdiagonal>true</allowdiagonal>
        <cutcorners>false</cutcorners>
        <allowsqueeze>false</allowsqueeze>
    </algorithm>
    <options>
        <loglevel>1</loglevel>
        <logpath/>
        <logfilename/>
    </options>
    <log>
        <mapfilename>../../Examples/example1.xml</mapfilename>
        <summary numberofsteps="3" nodescreated="2" length="2" length_scaled="48" time="0.000015"/>
        <path>
            <row number="0">1 * </row>
            <row number="1">* * </row>
        </path>
        <lplevel>
            <node x="0" y="1" number="0"/>
            <node x="1" y="1" number="1"/>
            <node x="1" y="0" number="2"/>
        </lplevel>
        <hplevel>
            <section number="0" start.x="0" start.y="1" finish.x="1" finish.y="1" length="1"/>
            <section number="1" start.x="1" start.y="1" finish.x="1" finish.y="0" length="1"/>
        </hplevel>
    </log>
</root>
